@page "/"

@inject IDemo _demo
@inject IEnumerable<IDemo> _demos

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

@foreach (var demo in _demos)
{
    
    @if (demo is ILocalDemo)
    {
        <p>Demo: @demo.StartupTime</p>
    }

    @if (demo is IUtcDemo)
    {
        <p>Utc Demo: @demo.StartupTime</p>

    }
}
